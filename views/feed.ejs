      <div class="continer  d-flex" style=" flex-direction: row;  justify-content: space-evenly;" >
        <% allusers.forEach(function(val){  %>
        <div class="card" style="width: 18rem; display: flex;">
          <img src="./images/uploads/<%= val.image %>" class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title"><%= val.username %></h5>
              <p class="card-text"><%= val.email %></p>
              <small>Like by <span><%= val.like %></span> people</small>
              <a href="/like/<%= val._id %>" style="text-decoration: none; margin-left: 20px;" id="Likes" ><i class="ri-heart-line" style="font-size: 35px; color: black;"></i> </a>
              <a href="/follow/<%= val._id %>" class="btn btn-primary" style="background-color: limegreen;  margin-left: 40px; border: none; margin-bottom: 14px;" > <i class="ri-user-follow-line" ></i></a>
              <a href="/oneUser/<%= val._id %>" class="btn btn-primary"> View Profile</a>
              </div>
            <%  })  %>
         </div>
      </div>
